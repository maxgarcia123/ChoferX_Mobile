    },
    "android": {
      "package": "com.max_garcia.mobi",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCIOPZMR8tzQ0DCYKXTIsYwvqP6XS2GdUs"
        }
      }
    }
  }
